; Chose one of the patterns randomly
(define (create-pattern-grid)
  (let ([rand_n (random 3)])
    (cond
      [(= rand_n 0) gliders]
      [(= rand_n 1) penta-decathlon]
      [(= rand_n 2) pulsar])))

(provide (all-defined-out))